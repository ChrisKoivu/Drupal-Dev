<?php

/**
 * @file
 * Install, update and uninstall functions for the Site Name Change module.
 */

/**
 * Implements hook_install().
 */
function site_name_change_install() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

/**
 * Implements hook_uninstall().
 */
function site_name_change_uninstall() {
  \Drupal::messenger()->addStatus(__FUNCTION__);
}

